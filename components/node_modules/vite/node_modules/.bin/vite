#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/rolldown-vite@7.1.14_@types+node@24.8.1/node_modules/rolldown-vite/bin/node_modules:/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/rolldown-vite@7.1.14_@types+node@24.8.1/node_modules/rolldown-vite/node_modules:/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/rolldown-vite@7.1.14_@types+node@24.8.1/node_modules:/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/rolldown-vite@7.1.14_@types+node@24.8.1/node_modules/rolldown-vite/bin/node_modules:/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/rolldown-vite@7.1.14_@types+node@24.8.1/node_modules/rolldown-vite/node_modules:/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/rolldown-vite@7.1.14_@types+node@24.8.1/node_modules:/mnt/d/A_test_code/monerepo-Test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
